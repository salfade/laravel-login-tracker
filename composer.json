{
    "name": "salfade/laravel-login-tracker",
    "description": "A simple package to track users successful login attempts",
    "keywords": [
        "spatie",
        "laravel-login-tracker"
    ],
    "homepage": "https://github.com/spatie/laravel-login-tracker",
    "license": "MIT",
    "authors": [
        {
            "name": "Mohamed Fawzan",
            "email": "fawzanm@gmail.com",
            "homepage": "https://salfade.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.0",
        "psalm/plugin-laravel": "^1.2",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Salfade\\LoginTracker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Salfade\\LoginTracker\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Salfade\\LoginTracker\\LoginTrackerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
